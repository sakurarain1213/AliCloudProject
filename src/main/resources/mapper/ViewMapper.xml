<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hou.mapper.ViewMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.hou.entity.View">
        <id column="id" property="id" />
        <result column="customID" property="customID" />
        <result column="houseID" property="houseID" />
        <result column="date" property="date" />
        <result column="notes" property="notes" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, customID, houseID, date, notes
    </sql>

</mapper>
